                <% singers.map((singer,i)=>{ %>

                <div class="col-lg-3 col-md-6">
                  <div class="team">
                    <a href="/checkout/<%= singer._id %>">
                      <div
                        class="team-image"
                        style="
                          position: relative;
                          background-image: url('/singers/image/<%= singer.imageName %>');
                          height: 400px;
                          background-repeat: no-repeat;
                          background-size: cover;
                          background-position: center;
