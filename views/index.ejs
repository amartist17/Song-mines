  <body class="stretched">
    <div id="wrapper" class="clearfix">
      <%- include('header'); -%>
      <section
        id="slider"
        class="slider-element slider-parallax swiper_wrapper min-vh-60 min-vh-md-100 include-header"
      >
        <div class="slider-inner">
          <div class="swiper-container swiper-parent">
            <div class="swiper-wrapper">
              <div class="swiper-slide dark">
                <div class="container">
                  <div class="slider-caption slider-caption-center">
                    <h2 data-animate="fadeInUp">Songmines</h2>
                    <p
                      class="d-none d-sm-block"
                      data-animate="fadeInUp"
                      data-delay="200"
                    >
                      Unique music for unique moments: Let's create something
                      amazing. <br />
                      Our artists will compose for an occassion starting at
                      6000/-
                    </p>
                    <a
                      href="#about"
                      class="button button-border button-circle action-btn"
                      data-animate="fadeInUp"
                      data-delay="200"
                      >How it works</a
                    >
                  </div>
                </div>
                <div
                  class="swiper-slide-bg"
                  style="
                    background-image: url('/homepage/slider2.png');
                    background-position: top center;
                  "
                ></div>
              </div>

              <div class="swiper-slide dark">
                <div class="container">
                  <div class="slider-caption slider-caption-center">
                    <h2 data-animate="fadeInUp">Beautifully Flexible</h2>
